{
  "manifest_version": 2,

  "name": "Time to 60fps",
  "description": "This extension shows the time it takes for a web page to reach 60fps",
  "version": "1.1",
  "browser_action": {
    "default_icon": "60fps-19x19.png"
  },
  "icons": {
    "48": "60fps-48x48.png",
    "128": "60fps-128x128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["time-to-60fps.js"],
      "run_at": "document_start"
    }
  ]
}

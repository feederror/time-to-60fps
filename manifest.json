{
  "manifest_version": 2,

  "name": "Time to 60fps",
  "description": "This extension shows the time it takes for a web page to reach 60fps",
  "version": "1.0",
  "browser_action": {
    "default_icon": "60fps.png"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["time-to-60fps.js"]
    }
  ]
}